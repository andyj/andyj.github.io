<!DOCTYPE html>
<html lang="en">
<head>

  <meta charset="utf-8">
  <meta name="description" content="">
  <meta name="author" content="Andy Jarrett">
  <meta name="robots" content="index, follow">
  <meta itemprop="creator" content="Andy Jarrett">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <meta name="viewport" content="width=device-width, initial-scale=1">
<!--   <meta itemprop="description" content=" by Andy Jarrett"> -->
  <meta itemprop="name" content="Blog of Andy Jarrett - Node.JS, tech, Railo, CFML and web stuff">
  <meta name="google-site-verification" content="CDqaY_sxw7luN0EPtFpOxiaJtIv9gog2sMAaT4pU-EI" />

  <title><%= @getPreparedTitle() %></title>
  <%- @getBlock("meta").toHTML() %>
  <!-- Style 'n stuff -->
  <link rel="shortcut icon" type="image/x-icon" href="/favicon.ico"/>
  <link rel="alternate" type="application/rss+xml" title="RSS" href="/rss.xml" />
  <link href="//maxcdn.bootstrapcdn.com/bootswatch/3.2.0/lumen/bootstrap.min.css" rel="stylesheet">
  <link href="//maxcdn.bootstrapcdn.com/font-awesome/4.1.0/css/font-awesome.min.css" rel="stylesheet">
  <%- @getBlock("styles").add(["/stylesheets/default.css"]).toHTML() %>
  <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
  <!--[if lt IE 9]>
  <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
  <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
  <![endif]-->
</head>
<body>
<div class="container-fluid">
  <div class="row">
    <div class="col-sm-4 col-md-3 sidebar">
      <h3>Posts</h3>
      <ul class="nav nav-sidebar">
        <% for post in @getCollection("posts").toJSON(): %>
        <!-- first it uses the getCollection template helper to fetch the html collection, which is a
          pre-defined collection by DocPad that contains all the HTML documents in our website.  -->
        <li class="<%= if post.id is @document.id then 'active' else 'inactive' %>">
          <a href="<%= post.url %>"> <%= post.title %> </a>
        </li>
        <% end %>
      </ul>
      <h3>Find me</h3>
      <ul class="list-group">
        <li class="list-group-item"><i class="fa fa-twitter-square"></i> <a rel="me nofollow" href="http://www.twitter.com/andyj">Twitter @andyj</a></li>
        <li class="list-group-item"><i class="fa fa-git-square"></i> <a rel="me nofollow" href="https://github.com/andyj">Github @andyj</a></li>
        <li class="list-group-item"><i class="fa fa-instagram"></i> <a rel="me nofollow" href="http://instagram.com/andyjarrett">Instagram @andyj</a></li>
        <li class="list-group-item"><i class="fa fa-skype"></i> <a rel="me nofollow" href="skype:andy.jarrett?call">Skype andy.jarrett</a></li>
        <li class="list-group-item"><i class="fa fa-envelope-square"></i> <a rel="me nofollow">mail@andyjarrett.co.uk</a></li>
        <!-- <li class="list-group-item"><i class="fa fa-rss-square"></i> <a href="http://feeds.feedburner.com/andyjarrett">RSS</a></li> -->
        <li class="list-group-item"><i class="fa fa-rss-square"></i> <a href="http://feeds.feedburner.com/andyjarrett">RSS</a></li>
      </ul>
      <ul class="nav nav-sidebar">
        <% for page in @getCollection("pages").toJSON(): %>
        <!-- first it uses the getCollection template helper to fetch the html collection, which is a
          pre-defined collection by DocPad that contains all the HTML documents in our website.  -->
        <li class="<%= if page.id is @document.id then 'active' else 'inactive' %>">
        <article itemprop="blogPosts" itemscope itemtype="http://schema.org/BlogPosting"></article>
          <a href="<%= page.url %>">
            <%= page.title %>
          </a>
        </li>
        <% end %>
      </ul>
    </div>
    <div class="col-sm-8 col-sm-offset-4 col-md-9 col-md-offset-3 main">
      <div class="page-header">
        <h1>Andy Jarrett <small>Web.App.Dev etc</small></h1>
      </div>
      <%- @content %>
    </div>
  <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
  <%- @getBlock("scripts").add(["/scripts/jquery-1.11.1.min.js", "//maxcdn.bootstrapcdn.com/bootstrap/3.2.0/js/bootstrap.min.js", "/scripts/default.js"]).toHTML() %>
</body>
</html>